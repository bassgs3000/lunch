<% unless notice.blank? %>
	<p class="alert alert-success"><%= notice %></p>
<% end %>
<h1 class="offset1">Preference for <%= current_user.name %></h1>
<div class="hero-unit">

<p>
  <b>Likes:</b>
  <%= @preference.likes %>
</p>

<p>
  <b>Dislikes:</b>
  <%= @preference.dislikes %>
</p>


<%= link_to 'Edit', edit_preference_path(@preference) %> |
<%= link_to 'Back', preferences_path %>
</div>
